@page
@model IndexModel

<table class="table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Date</th>
            <th>Description</th>           
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.List!)
        {
            <tr>
                <td><a asp-page="Details" asp-route-id="@item.Id">@item.ServiceDate</a></td>
                <td>@(item.Description??"Service")</td>
               
            </tr>
        }
    </tbody>
</table>

@section Sidebar{}
