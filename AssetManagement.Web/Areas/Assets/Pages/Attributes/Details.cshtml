@page "{id}"
@model DetailsModel

<div class="row">
    <div class="col-md">
        <div class="card">
            <div class="card-body">
              
                <div class="row">
                    <label class="col-md-3">Name</label>
                    <div class="col-md-9">@Model.Attribute.Name</div>
                </div>
                <div class="row mt-3">
                    <label class="col-md-3">Expiry date</label>
                    <div class="col-md-9">@Model.Attribute.ExpiryDate?.ToString("dd / MMM / yyyy")</div>
                </div> 
                                     
            </div>
        </div>
    </div>
</div>

@section Sidebar{
   
}
