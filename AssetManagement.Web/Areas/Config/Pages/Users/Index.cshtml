@page
@model IndexModel
<table class="table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Mobile</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.List!)
        {
            <tr>
                <td><a asp-page="Details" asp-route-id="@item.Id">@item.Name</a></td>
                <td>@item.Email</td>
                <td>@item.Mobile</td>
            </tr>
        }
    </tbody>
</table>